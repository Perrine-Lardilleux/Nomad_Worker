<% if user_signed_in? && current_user.wishlists?(@city) %>
  <% #raise %>
  <%= button_to "Remove from wishlist", city_wishlist_path(@city), method: :delete, class:"btn-danger", remote: true %>
<% else %>
  <%= button_to "Add to wishlist", city_wishlists_path(@city), class:"btn-success", remote: true %>
<% end %>
